{
  "cli": {
    "version": ">= 16.28.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "EXPO_PUBLIC_INTERNAL_TESTING": "true"
      }
    },
    "appstore": {
      "autoIncrement": true
    }
  },
  "submit": {
    "production": {},
    "appstore": {}
  }
}
